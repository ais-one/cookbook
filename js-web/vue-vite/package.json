{
  "name": "vue-vite",
  "version": "0.0.1",
  "description": "vue 3 SPA + vite example",
  "author": "aaronjxz@gmail.com",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .js,.vue",
    "prettier:check": "prettier --check src/**/*.{js,jsx,json}",
    "prettier:write": "prettier --write src/**/*.{js,jsx,json}",
    "api:dev": "npm run app --prefix ../../js-node/expressjs -- development",
    "dev": "vite",
    "build": "vite build",
    "test:e2e": "echo \"Remember to run frontend and backend before test...\" && cypress run",
    "test:e2e:quiet": "echo \"Remember to run frontend and backend before test...\" && cypress run --quiet --reporter json",
    "test:all:dev": "start-test api:dev http://localhost:3000/api/healthcheck dev http://localhost:3000 test:e2e"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@antv/data-set": "^0.11.8",
    "@antv/g2": "^4.1.28",
    "@antv/g2plot": "^2.3.33",
    "@sentry/tracing": "^6.13.2",
    "@sentry/vue": "^6.13.2",
    "ant-design-vue": "^2.2.8",
    "echarts": "^5.2.1",
    "leaflet": "^1.7.1",
    "pinia": "^0.5.4",
    "rxjs": "^7.3.0",
    "vue": "^3.2.18",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.2",
    "@vue/compiler-sfc": "^3.2.18",
    "cypress": "^8.4.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.18.0",
    "prettier": "^2.4.1",
    "start-server-and-test": "^1.14.0",
    "vite": "^2.5.10"
  }
}
