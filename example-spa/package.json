{
  "name": "example-spa",
  "version": "0.0.0",
  "description": "A rest backend example for vue-crud-x component usage",
  "author": "Aaron Gong <aaronjxz@gmail.com>",
  "private": true,
  "scripts": {
    "deploy": "firebase deploy --only hosting",
    "build-rest": "cd ../backend&&npm i",
    "init-db": "cd ../backend&&npm run-script init-db",
    "rest": "cd ../backend&&npm run-script dev",
    "serve": "vue-cli-service serve --port 8080",
    "prod": "vue-cli-service build --mode production && scp -r -i ../../test.pem dist/* ubuntu@127.0.0.3:~/app/public",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "concurrently \"npm run rest\" \"npm run serve\""
  },
  "dependencies": {
    "@firebase/app": "^0.4.15",
    "@firebase/auth": "^0.11.8",
    "@firebase/firestore": "^1.5.0",
    "@firebase/storage": "^0.3.10",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "^1.0.18",
    "apollo-link-http": "^1.5.15",
    "apollo-link-ws": "^1.0.18",
    "apollo-server": "^2.9.3",
    "apollo-utilities": "^1.3.2",
    "axios": "^0.18.1",
    "core-js": "^2.6.5",
    "date-fns": "^1.30.1",
    "graphql": "^14.5.4",
    "graphql-tag": "^2.10.1",
    "json2csv": "^4.5.2",
    "jwt-decode": "^2.2.0",
    "lit-element": "^2.2.1",
    "lodash.orderby": "^4.6.0",
    "mongodb-stitch-browser-sdk": "^4.5.0",
    "rxjs": "^6.5.2",
    "subscriptions-transport-ws": "^0.9.16",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-rc.2",
    "vue-i18n": "^8.14.0",
    "vue-native-websocket": "^2.0.13",
    "vue-recaptcha": "^1.2.0",
    "vue-router": "^3.1.3",
    "vue-rx": "^6.2.0",
    "vuetify": "^2.0.11",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@mdi/font": "^3.9.97",
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "concurrently": "^4.1.2",
    "deepmerge": "^3.3.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "fibers": "^4.0.1",
    "node-sass": "^4.12.0",
    "sass": "^1.22.10",
    "sass-loader": "^7.2.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "ignored-browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
