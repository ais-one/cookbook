{
  "name": "example-app",
  "version": "0.5.1",
  "description": "example express application",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development PORT=3001 DEBUG=app:* nodemon --ignore tests/ --ignore public/ --watch src bin/www",
    "knex": "node deploy/db/knex/index.js",
    "mongo": "node deploy/db/mongo/index.js",
    "app": "nodemon --trace-warnings index.js",
    "app:lint": "nodemon --ignore tests/ --ignore public/ --exec \"npm run lint && node\" index.js",
    "process-cron": "node process-cron.js",
    "process-long": "node process-long.js",
    "test": "cross-env NODE_ENV=development jest --config=jest.config.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint --fix --no-fix-warnings --no-report-warnings ."
  },
  "dependencies": {
    "@es-labs/esm": "file:../@es-labs/esm",
    "@es-labs/node": "file:../@es-labs/node",
    "apollo-server-express": "^2.25.0",
    "axios": "^0.21.1",
    "csv-parse": "^4.15.4",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-json-validator-middleware": "^2.1.1",
    "express-oas-generator": "^1.0.38",
    "graphql": "^15.5.0",
    "json2csv": "^5.0.6",
    "kafkajs": "^1.15.0",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "selfsigned": "^1.10.11"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "node-mocks-http": "^1.10.1",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-commit-with-fix": "npm run lint && npm run lint:fix",
      "pre-commit-without-fix": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "vue-crud-x"
  },
  "keywords": [
    "express"
  ],
  "author": "aaronjxz@gmail.com",
  "license": "MIT"
}
