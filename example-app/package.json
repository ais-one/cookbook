{
  "name": "example-app",
  "version": "0.4.6",
  "description": "example express application",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "config": {
    "env": "development"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./bin/www",
    "dev": "cross-env NODE_ENV=development PORT=3001 DEBUG=app:* nodemon --ignore tests/ --ignore public/ --watch src bin/www",
    "knex": "cross-env NODE_ENV=%npm_package_config_env% node deploy/db/knex/migrations/index.js&&cross-env NODE_ENV=%npm_package_config_env% node deploy/db/knex/seeds/index.js",
    "knex:unix": "cross-env NODE_ENV=$npm_package_config_env node deploy/db/knex/migrations/index.js&&cross-env NODE_ENV=$npm_package_config_env node deploy/db/knex/seeds/index.js",
    "mongo": "cross-env NODE_ENV=%npm_package_config_env% node deploy/db/mongo/index.js",
    "mongo:unix": "cross-env NODE_ENV=$npm_package_config_env node deploy/db/mongo/index.js",
    "app": "cross-env NODE_ENV=%npm_package_config_env% nodemon --trace-warnings index.js",
    "app:lint": "cross-env NODE_ENV=%npm_package_config_env% nodemon index.js --ignore tests/ --ignore public/ --exec \"npm run lint && node\"",
    "app:unix": "cross-env NODE_ENV=$npm_package_config_env nodemon index.js  --ignore tests/ --ignore public/ --exec \"npm run lint && node\"",
    "process-cron": "cross-env NODE_ENV=%npm_package_config_env% node process-cron.js",
    "process-cron:unix": "cross-env NODE_ENV=$npm_package_config_env node process-cron.js",
    "process-long": "cross-env NODE_ENV=%npm_package_config_env% node process-long.js",
    "process-long:unix": "cross-env NODE_ENV=$npm_package_config_env node process-long.js",
    "test": "cross-env NODE_ENV=%npm_package_config_env% jest --config=jest.config.js",
    "test:unix": "cross-env NODE_ENV=$npm_package_config_env jest --config=jest.config.js",
    "deploy": "deploy.sh %npm_package_config_env%",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint --fix --no-fix-warnings --no-report-warnings ."
  },
  "dependencies": {
    "@es-labs/esm": "file:../@es-labs/esm",
    "@es-labs/node": "file:../@es-labs/node",
    "apollo-server-express": "^2.21.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csv-parse": "^4.15.1",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-oas-generator": "^1.0.30",
    "graphql": "^15.5.0",
    "helmet": "^4.4.1",
    "http-proxy-middleware": "^1.0.6",
    "json2csv": "^5.0.6",
    "kafkajs": "^1.15.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-saml": "^2.0.5",
    "selfsigned": "^1.10.8"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "node-mocks-http": "^1.10.1",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-commit-with-fix": "npm run lint && npm run lint:fix",
      "pre-commit-without-fix": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "vue-crud-x"
  },
  "keywords": [
    "express"
  ],
  "author": "aaronjxz@gmail.com",
  "license": "MIT"
}
