{
  "name": "vue-crud-x",
  "version": "0.3.3",
  "description": "Library for JS Application",
  "author": "",
  "license": "ISC",
  "config": {
    "env": "development",
    "app": "example-app",
    "web": "example-web"
  },
  "scripts": {
    "install-libs": "npm i && cd example-web/spa && npm i",
    "mongo": "cross-env NODE_ENV=%npm_package_config_env% APP_NAME=example-app node example-app/db/mongo/index.js",
    "mongo:unix": "cross-env NODE_ENV=$npm_package_config_env APP_NAME=example-app node example-app/db/mongo/index.js",
    "knex": "cross-env NODE_ENV=%npm_package_config_env% npx knex migrate:latest --knexfile=example-app/knexfile.js && cross-env NODE_ENV=%npm_package_config_env% npx knex seed:run --knexfile=example-app/knexfile.js",
    "knex:unix": "cross-env NODE_ENV=$npm_package_config_env npx knex migrate:latest --knexfile=example-app/knexfile.js && cross-env NODE_ENV=$npm_package_config_env npx knex seed:run --knexfile=example-app/knexfile.js",
    "spa": "cd example-web/spa && npm run serve",
    "vite": "cd example-web/vite && npm run dev",
    "app": "cross-env NODE_ENV=%npm_package_config_env% APP_NAME=example-app npx nodemon example-app/index.js",
    "app:unix": "cross-env NODE_ENV=$npm_package_config_env APP_NAME=example-app npx nodemon example-app/index.js",
    "process-long": "cross-env NODE_ENV=%npm_package_config_env% APP_NAME=example-app node example-app/process-long.js",
    "process-long:unix": "cross-env NODE_ENV=$npm_package_config_env APP_NAME=example-app node example-app/process-long.js",
    "process-cron": "cross-env NODE_ENV=%npm_package_config_env% APP_NAME=example-app node example-app/process-cron.js",
    "process-cron:unix": "cross-env NODE_ENV=$npm_package_config_env APP_NAME=example-app node example-app/process-cron.js",
    "app:spa": "concurrently \"npm run app\" \"npm run spa\"",
    "app:spa:unix": "concurrently \"npm run app:unix\" \"npm run spa\"",
    "test": "cross-env NODE_ENV=%npm_package_config_env% APP_NAME=example-app jest --config=example-app/jest.config.js --watchAll",
    "test:unix": "cross-env NODE_ENV=$npm_package_config_env APP_NAME=example-app jest --config=example-app/jest.config.js --watchAll",
    "deploy": "deploy.sh %npm_package_config_env% example-app example-web",
    "deploy:unix": "deploy.sh $npm_package_config_env example-app example-web",
    "lint": "eslint ."
  },
  "engines": {
    "node": "12"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.2.0",
    "agenda": "^3.0.0",
    "apollo-server-express": "^2.16.1",
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.4",
    "csv-parse": "^4.12.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "helmet": "^4.1.0",
    "http-proxy-middleware": "^1.0.5",
    "json2csv": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "keyv": "^4.0.1",
    "knex": "^0.21.5",
    "mongodb": "^3.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemon": "^2.0.4",
    "objection": "^2.2.3",
    "otplib": "^12.0.1",
    "passport": "^0.4.1",
    "passport-saml": "^1.3.4",
    "selfsigned": "^1.10.7",
    "sqlite3": "^5.0.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "cypress": "^5.0.0",
    "eslint": "^7.7.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.4.2",
    "node-mocks-http": "^1.9.0",
    "supertest": "^4.0.2"
  },
  "private": true
}
