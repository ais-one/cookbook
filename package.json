{
  "name": "cookbook",
  "version": "0.6.12",
  "description": "Fullstack JS Application Cookbook",
  "author": "aaronjxz@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ais-one/cookbook.git"
  },
  "bugs": {
    "url": "https://github.com/ais-one/cookbook/issues"
  },
  "homepage": "https://github.com/ais-one/cookbook#readme",
  "keywords": [
    "nodejs",
    "express",
    "vuejs",
    "reactjs"
  ],
  "scripts": {
    "prepare": "git config core.hooksPath ./git-hooks || echo 'Can not set git hooks'",
    "git:hash": "git rev-parse --verify --short HEAD",
    "git:branch": "git rev-parse --abbrev-ref HEAD",
    "ex:start": "NODE_ENV=development npm run app --workspace=js-node/expressjs",
    "ex:start:win": "set NODE_ENV=development&&npm run app --workspace=js-node/expressjs",
    "ex:build": "npm i --workspace=js-node/expressjs",
    "aa": "set EDITOR=vim && echo $EDITOR",
    "ex:lib:build": "npm i @es-labs/node@latest --workspace=js-node/expressjs"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "workspaces": [
    "js-node/*",
    "js-libs/*",
    "js-tools/*",
    "js-web/*"
  ],
  "private": true
}
