{
  "name": "vue-crud-x",
  "version": "0.3.6",
  "description": "Library for JS Application",
  "author": "",
  "license": "MIT",
  "config": {
    "env": "development",
    "app": "example-app",
    "web": "example-web"
  },
  "scripts": {
    "install-libs": "npm i && cd example-web/spa && npm i",
    "mongo": "cd example-app&&npm run mongo",
    "mongo:unix": "cd example-app&&npm run mongo:unix",
    "knex": "cd example-app&&npm run knex",
    "knex:unix": "cd example-app&&npm run knex:unix",
    "spa": "cd example-web/spa && npm run serve",
    "vite": "cd example-web/vite && npm run dev",
    "app": "cd example-app&&npm run app",
    "app:unix": "cd example-app&&npm run app:unix",
    "process-cron": "cd example-app&&npm run process-cron",
    "process-cron:unix": "cd example-app&&npm run process-cron:unix",
    "process-long": "cd example-app&&npm run process-long",
    "process-long:unix": "cd example-app&&npm run process-long:unix",
    "app:spa": "concurrently \"npm run app\" \"npm run spa\"",
    "app:spa:unix": "concurrently \"npm run app:unix\" \"npm run spa\"",
    "test": "cd example-app&&npm run test",
    "test:unix": "cd example-app&&npm run test:unix",
    "deploy": "deploy.sh %npm_package_config_env% example-app example-web",
    "deploy:unix": "deploy.sh $npm_package_config_env example-app example-web"
  },
  "engines": {
    "node": "12"
  },
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "private": true
}
