{
  "name": "app",
  "description": "Generic Application Template",
  "config": {
    "env": "production",
    "app": "example-app"
  },
  "scripts": {
    "install-libs": "npm i && cd %npm_package_config_app% && npm i && cd web/spa && npm i",
    "install-db": "cd %npm_package_config_app% && npm run install-db",
    "spa": "cd %npm_package_config_app%/web/spa && cross-env NODE_ENV=%npm_package_config_env% npm run serve",
    "dev:spa": "concurrently \"npm run dev\" \"npm run spa\"",
    "dev": "cross-env NODE_ENV=development APPNAME=%npm_package_config_app% nodemon index.js",
    "test": "cross-env NODE_ENV=%npm_package_config_env% APPNAME=%npm_package_config_app% jest --watchAll",
    "build": "build.sh %npm_package_config_app% %npm_package_config_env%",
    "deploy": "deploy.sh %npm_package_config_app% %npm_package_config_env%",
    "lint": "eslint ."
  },
  "engines": {
    "node": "12"
  },
  "main": "common-web/dist/VueCrudX.js",
  "dependencies": {
    "agenda": "^3.0.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bull": "^3.14.0",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.4",
    "csv-parse": "^4.9.1",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "firebase-admin": "^8.12.1",
    "http-proxy-middleware": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "keyv": "^4.0.1",
    "knex": "^0.21.1",
    "mongodb": "^3.5.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "objection": "^2.1.3",
    "otplib": "^12.0.1",
    "passport": "^0.4.1",
    "passport-saml": "^1.3.3",
    "sqlite3": "^4.2.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.0.1",
    "node-mocks-http": "^1.8.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  },
  "private": true
}
