{
  "name": "vue-crud-x",
  "version": "0.3.2",
  "description": "Library for JS Application",
  "author": "",
  "license": "ISC",
  "config": {
    "env": "development",
    "app": "example-app",
    "web": "example-web"
  },
  "scripts": {
    "install-libs": "npm i && cd %npm_package_config_app% && npm i && cd web/spa && npm i",
    "install-db": "cd %npm_package_config_app% &&cross-env NODE_ENV=%npm_package_config_env% npm run install-db",
    "spa": "cd %npm_package_config_app%/web/spa && cross-env NODE_ENV=%npm_package_config_env% npm run serve",
    "dev": "cross-env NODE_ENV=development APPNAME=%npm_package_config_app% nodemon common-lib/index.js",
    "dev:spa": "concurrently \"npm run dev\" \"npm run spa\"",
    "test": "cross-env NODE_ENV=%npm_package_config_env% APPNAME=%npm_package_config_app% jest --config=%npm_package_config_app%/jest.config.js --watchAll",
    "deploy:be": "deploy-be.sh %npm_package_config_app% %npm_package_config_env%",
    "deploy:fe": "deploy-fe.sh %npm_package_config_web% %npm_package_config_env%",
    "lint": "eslint ."
  },
  "engines": {
    "node": "12"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.1.1",
    "agenda": "^3.0.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.4",
    "csv-parse": "^4.10.1",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "http-proxy-middleware": "^1.0.4",
    "json2csv": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "keyv": "^4.0.1",
    "knex": "^0.21.1",
    "mongodb": "^3.5.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "objection": "^2.2.0",
    "otplib": "^12.0.1",
    "passport": "^0.4.1",
    "passport-saml": "^1.3.3",
    "selfsigned": "^1.10.7",
    "sqlite3": "^4.2.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "cypress": "^4.9.0",
    "eslint": "^7.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.1.0",
    "node-mocks-http": "^1.8.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  },
  "private": true
}
