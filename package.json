{
  "name": "app",
  "description": "Backend application",
  "config": {
    "app": "example-app"
  },
  "scripts": {
    "install-libs": "npm i && cd %npm_package_config_app%/web/spa && npm i && cd ../ssr && npm i",
    "install-db": "cd %npm_package_config_app% && npm run install-db",
    "test": "cross-env NODE_ENV=development jest --watchAll",
    "dev": "cross-env NODE_ENV=development nodemon index.js -- %npm_package_config_app%",
    "spa": "cd %npm_package_config_app%/web/spa && cross-env NODE_ENV=development npm run serve",
    "ssr": "cd %npm_package_config_app%/web/ssr && cross-env NODE_ENV=development npm run dev",
    "dev:spa": "concurrently \"npm run dev\" \"npm run spa\"",
    "deploy": "cd %npm_package_config_app%/deploy && run.sh",
    "lint": "eslint ."
  },
  "engines": {
    "node": "12"
  },
  "dependencies": {
    "@keyv/redis": "^2.1.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.10.0",
    "http-proxy-middleware": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "keyv": "^4.0.0",
    "knex": "^0.20.11",
    "mongodb": "^3.5.5",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "objection": "^2.1.3",
    "otplib": "^12.0.1",
    "passport": "^0.4.1",
    "passport-saml": "^1.3.3",
    "sqlite3": "^4.1.1",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.3",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^25.1.0",
    "node-mocks-http": "^1.8.1",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "private": true
}
