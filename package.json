{
  "name": "vue-crud-x",
  "version": "0.3.2",
  "description": "Library for JS Application",
  "author": "",
  "license": "ISC",
  "config": {
    "env": "development",
    "app": "example-app",
    "web": "example-web"
  },
  "scripts": {
    "install-libs": "npm i && cd %npm_package_config_web%/spa && npm i",
    "initdb-mongo": "cross-env NODE_ENV=%npm_package_config_env% node %npm_package_config_app%/db/mongo/seed.js",
    "initdb-knex": "cross-env NODE_ENV=%npm_package_config_env% npx knex migrate:latest --knexfile=%npm_package_config_app%/knexfile.js && cross-env NODE_ENV=%npm_package_config_env% npx knex seed:run --knexfile=%npm_package_config_app%/knexfile.js",
    "spa": "cd %npm_package_config_web%/spa && cross-env NODE_ENV=%npm_package_config_env% npm run serve",
    "app": "cross-env NODE_ENV=%npm_package_config_env% APP_NAME=%npm_package_config_app% npx nodemon %npm_package_config_app%/index.js",
    "process-long": "cross-env NODE_ENV=%npm_package_config_env% APP_NAME=%npm_package_config_app% node %npm_package_config_app%/process-long.js",
    "process-cron": "cross-env NODE_ENV=%npm_package_config_env% APP_NAME=%npm_package_config_app% node %npm_package_config_app%/process-cron.js",
    "app:spa": "concurrently \"npm run app\" \"npm run spa\"",
    "test": "cross-env NODE_ENV=%npm_package_config_env% APP_NAME=%npm_package_config_app% jest --config=%npm_package_config_app%/jest.config.js --watchAll",
    "deploy": "deploy.sh %npm_package_config_app% %npm_package_config_env% %npm_package_config_web%",
    "lint": "eslint ."
  },
  "engines": {
    "node": "12"
  },
  "dependencies": {
    "@google-cloud/firestore": "^4.1.1",
    "@google-cloud/storage": "^5.1.1",
    "agenda": "^3.0.0",
    "apollo-server-express": "^2.15.1",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.4",
    "csv-parse": "^4.10.1",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "helmet": "^3.23.3",
    "http-proxy-middleware": "^1.0.5",
    "json2csv": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "keyv": "^4.0.1",
    "knex": "^0.21.2",
    "mongodb": "^3.5.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemon": "^2.0.4",
    "objection": "^2.2.0",
    "otplib": "^12.0.1",
    "passport": "^0.4.1",
    "passport-saml": "^1.3.3",
    "selfsigned": "^1.10.7",
    "sqlite3": "^4.2.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "cypress": "^4.9.0",
    "eslint": "^7.4.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.1.0",
    "node-mocks-http": "^1.8.1",
    "supertest": "^4.0.2"
  },
  "private": true
}
