{
  "name": "example-rest",
  "version": "0.0.0",
  "description": "A rest backend example for vue-crud-x component usage",
  "author": "Aaron Gong <aaronjxz@gmail.com>",
  "private": true,
  "scripts": {
    "build-rest": "cd ../backend&&npm i",
    "init-db": "cd ../backend&&npm run-script init-db",
    "rest": "cd ../backend&&npm run-script dev",
    "serve": "vue-cli-service serve",
    "prod": "vue-cli-service build --mode production && scp -r -i ../../test.pem dist/* ubuntu@127.0.0.3:~/app/public",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "concurrently \"npm run rest\" \"npm run serve\""
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-http": "^1.5.14",
    "axios": "^0.18.0",
    "date-fns": "^1.30.1",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "jwt-decode": "^2.2.0",
    "lit-element": "^2.1.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "rxjs": "^6.4.0",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-beta.28",
    "vue-authenticate": "^1.3.4",
    "vue-i18n": "^8.10.0",
    "vue-native-websocket": "^2.0.13",
    "vue-router": "^3.0.3",
    "vue-rx": "^6.1.0",
    "vuetify": "^1.5.11",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.5",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-service": "^3.5.3",
    "@vue/eslint-config-standard": "^4.0.0",
    "concurrently": "^4.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.10"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "ignored-browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
