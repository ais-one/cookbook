{
  "name": "example-nuxt",
  "version": "0.0.0",
  "description": "My superior Nuxt.js project",
  "author": "Aaron Gong",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt",
    "serve": "npx http-server dist -p 8080",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "cross-env NODE_ENV=development nuxt generate --devtools",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "clean": "rm -rf dist&&rm -rf .nuxt",
    "build-rest": "cd ../backend&&npm i",
    "init-db": "cd ../backend&&npm run-script init-db",
    "rest": "cd ../backend&&npm run-script dev",
    "ssr": "concurrently \"npm run rest\" \"npm run dev\"",
    "static": "concurrently \"npm run rest\" \"npx http-server dist -p 8080\""
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.5.3",
    "@nuxtjs/axios": "^5.4.1",
    "@nuxtjs/dotenv": "^1.3.0",
    "cross-env": "^5.2.0",
    "http-server": "^0.11.1",
    "nuxt": "^2.6.2",
    "nuxt-i18n": "^5.8.5",
    "rxjs": "^6.5.1",
    "vue-rx": "^6.1.0",
    "vuetify": "^1.5.13"
  },
  "devDependencies": {
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "nodemon": "^1.18.11",
    "prettier": "1.16.4",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
