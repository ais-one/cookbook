{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "ssh": "ssh -i ../xxx.pem -L 27017:127.0.0.1:27017 ubuntu@11.22.33.44",
    "lint": "eslint .",
    "test-tbd": "cross-env NODE_ENV=development jest --coverage --detectOpenHandles --forceExit --reporter=html --exclude __tests__ --watchAll",
    "test": "cross-env NODE_ENV=development jest --watchAll",
    "init-db": "npx knex migrate:latest --env development&&npx knex seed:run --env development",
    "i:spa": "cd ../example-spa&&npm i",
    "i:ssr": "cd ../example-ssr&&npm i",
    "del-db": "rm -f dev.sqlite3",
    "dev": "set NODE_ENV=development&&nodemon index.js",
    "dev:spa": "concurrently \"npm run dev\" \"cd ../example-spa&&npm run-script serve\"",
    "dev:ssr": "concurrently \"npm run dev\" \"cd ../example-ssr&&npm run-script dev\"",
    "dev:static": "concurrently \"npm run dev\" \"cd ../example-ssr&&npm run-script serve\"",
    "multiple-sample": "pm2 delete APP1 APP2 && pm2 start --only APP1,APP2 --env production",
    "prod:deploy": "mv node_modules ../node_modules_tmp&&scp -r -i ../../test.pem . ubuntu@127.0.0.1:~/api&&mv ../node_modules_tmp ./node_modules",
    "prod:list": "ssh -i ../../test.pem ubuntu@127.0.0.1 \"pm2 list\"",
    "prod:start": "ssh -i ../../test.pem ubuntu@127.0.0.1 \"cd ~/api; authbind --deep pm2 start --only api --env production;\"",
    "prod:stop": "ssh -i ../../test.pem ubuntu@127.0.0.1 \"cd ~/api; pm2 delete api;\"",
    "prod:nodemon": "sudo NODE_ENV=production nodemon server.js",
    "prod": "npm run prod:stop&&npm run prod:deploy&&npm run prod:start"
  },
  "engines": {
    "node": "8"
  },
  "dependencies": {
    "@keyv/redis": "^2.1.0",
    "apollo-server-express": "^2.9.16",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.9.2",
    "graphql": "^14.6.0",
    "helmet": "^3.21.2",
    "http-proxy-middleware": "^0.20.0",
    "ioredis": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "keyv": "^4.0.0",
    "knex": "^0.20.8",
    "mongodb": "^3.5.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "objection": "^2.1.2",
    "otplib": "^12.0.1",
    "passport": "^0.4.1",
    "passport-saml": "^1.2.0",
    "qrcode": "^1.4.4",
    "sqlite3": "^4.1.1",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.3",
    "uuid": "^3.4.0",
    "ws": "^7.2.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^25.1.0",
    "node-mocks-http": "^1.8.1",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "private": true
}
